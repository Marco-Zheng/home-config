#other
alias vi=vim

#ssh
pi_ssh () { ~/.bin/sshlogin 10.0.0.11 pi 22 $password_pi }
lecheng_ssh () { ~/.bin/sshlogin 42.96.150.189 root 22 $password_lecheng }
dpproxy_ssh () { ~/.bin/sshlogin 192.168.8.172 yuyang.gong 22 $password_dpproxy }
dpbeta_ssh () { ~/.bin/sshlogin $1 betauser 58422 $password_dpbeta }
dppush_ssh () { ssh dp_readonly@$1 -p 58422 }
dpalpha_ssh () { ~/.bin/sshlogin $1 root 58422 $password_dpalpha }

#alias
kill_by_port () { lsof -i tcp:$1 | awk 'NR!=1 {print $2}' | xargs kill -9 }
http_proxy () { polipo socksParentProxy=localhost:1080 proxyAddress=0.0.0.0 }

#shortcut
mvn_dp () { cp ~/.m2/settings.xml.dp ~/.m2/settings.xml }
mvn_default () { rm ~/.m2/settings.xml }

lion_qa () { cp /data/webapps/appenv.qa /data/webapps/appenv }
lion_alpha () { cp /data/webapps/appenv.alpha /data/webapps/appenv }
lion_dev () { rm /data/webapps/appenv }
