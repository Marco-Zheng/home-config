[user]
	name = Dozer
	email = mail@dozer.cc
[color]
	status = auto
	diff = auto
	branch = auto
	interactive = auto
[core]
	quotepath = false
	mergeoptions = --no-edit
	editor = /usr/bin/vim
	trustctime = false
	excludesfile = ~/.gitignore_global
[push]
	default = simple
[mergetool]
	keepBackup = false
[include]
	path = ~/.gitconfig_local
[includeIf "gitdir:~/Projects/narvii/"]
  path = .gitconfig_narvii

[url "git@bitbucket.org:"]
	insteadOf = https://bitbucket.org/
